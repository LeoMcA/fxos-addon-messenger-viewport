{
    "manifest_version": 1,
    "name": "Messenger Viewport",
    "description": "Adds a viewport meta tag to messenger.com",
    "version": "0.0.2",
    "author": "Leo McArdle",
    "content_scripts": [{
      "matches": ["https://www.messenger.com/*"],
      "js": ["js/index.js"]
    }],
    "icons": {
      "128": "/icons/128.png"
    }
}
